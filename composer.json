    "license": "MIT",
    "homepage": "https://github.com/zonuexe/php-myteto",
    "require-dev": {
	"phpunit/phpunit": "4.0.*"
        ,"apigen/apigen": "2.*"
    },
    "authors": [
	{
	    "name": "USAMI Kenta",
	    "homepage": "http://dt.zonu.me/"
	},
	{
	    "name": "pixiv Inc.",
	    "homepage": "http://www.pixiv.co.jp/"
	}
    ],
    "config": {
        "vendor-dir": "vendor/composer"
